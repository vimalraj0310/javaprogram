public class Parent
{
final void print()
{
System.out.println("hi");

}
void prin()

{
System.out.println("hello");

}
}

public class Child extends Parent
{
void print(){
System.out.println("hi and hello");


}

}

public class Main
{
public static void main(String args[]){
Child scan=new Child();
scan.print();

}


}
