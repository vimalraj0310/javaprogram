public class Overload
{
int add(int a,int b)
{
int c=a+b;
System.out.println(c);
return c;
}
int add(int a,int b,int c)
{
int d=a+b+c;
System.out.println(d);
return d;
}
double add(double a,int b)
{
double c=a+b;
System.out.println(c);
return c;
}
double add(double a,double b)
{
double c=a+b;
System.out.println(c);
return c;
}
public static void main(String args[])
{
Overload obj=new Overload();
obj.add(5,5);
obj.add(5,5,5);
obj.add(80.0,5);
obj.add(10.5,10.5);

}



}
